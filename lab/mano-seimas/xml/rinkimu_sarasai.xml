	<rinkiminis_sarasas id="ID1" pavadinimas="Pavadinimas">
		<partija id="PartijosID1"/>
		<partija id="PartijosID1"/>
	</balsavimas>
	<rinkiminis_sarasas id="ID2" pavadinimas="Pavadinimas">
		<partija id="PartijosID3"/>
	</balsavimas>
